{
    "pylint.args": [
    ],
    // â†“ Change to "Hint" to not show spell checker in problems
    "cSpell.diagnosticLevel": "Information",
    "black-formatter.args": [
        "--skip-string-normalization",
        "--line-length=100"
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "autoDocstring": {
        "docstringFormat": "numpy"
    },
    "cSpell.words": [
        "astype",
        "autofilter",
        "autofit",
        "docstrings",
        "downcasting",
        "dtype",
        "dtypes",
        "dups",
        "eqlty",
        "idxs",
        "lgnd",
        "lsuffix",
        "processings",
        "rsuffix",
        "xlsxwriter"
    ]
}